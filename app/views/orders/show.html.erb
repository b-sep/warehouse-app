<h1>Pedido <%= @order.code %></h1>

<section>
  <div>
    <p>Galpão destino: <%= @order.warehouse.full_description %></p>
  </div>
  <div>
    <p>Fornecedor: <%= @order.supplier.full_description %></p>
  </div>
  <div>
    <p>Usuário responsável: <%= @order.user.name %> | <%= @order.user.email %> </p>
  </div>
  <div>
    <p>Data prevista de entrega: <%= I18n.localize(@order.estimated_delivery_date) %></p>
  </div>
</section>