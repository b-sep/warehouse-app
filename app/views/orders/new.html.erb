<h1>Registrar pedido</h1>

<%= form_with model: @order do |f| %>
  <fieldset>
    <%= f.label :warehouse_id %>
    <%= f.collection_select :warehouse_id, @warehouses, :id, :name %>
  </fieldset>

  <fieldset>
    <%= f.label :supplier_id %>
    <%= f.collection_select :supplier_id, @suppliers, :id, :corporate_name %>
  </fieldset>

   <fieldset>
    <%= f.label :estimated_delivery_date %>
    <%= f.text_field :estimated_delivery_date %>
  </fieldset>

  <%= f.submit 'Registrar' %>
<% end %>