<h1>Novo galpão</h1>

<%= form_with model: Warehouse.new do |f| %>
  <div>
    <%= f.label :name, 'Nome:' %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :code, 'Código:' %>
    <%= f.text_field :code %>
  </div>

  <div>
    <%= f.label :city, 'Cidade:' %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :area, 'Área:' %>
    <%= f.number_field :area %>
  </div>

  <div>
    <%= f.label :address, 'Endereço:' %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :zip_code, 'Cep:' %>
    <%= f.text_field :zip_code %>
  </div>

  <div>
    <%= f.label :description, 'Descrição:' %>
    <%= f.text_area :description %>
  </div>

  <%= f.submit 'Adicionar galpão' %>
<% end %>